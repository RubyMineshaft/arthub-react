<%= display_errors(@artwork) %>


<%= bootstrap_form_for([@user, @artwork]) do |f| %>

  <%= f.text_field :title %>

  <%= f.date_field :date %>

  <%= f.text_area :description %>

  <%= f.text_field :url, placeholder: "http://example.com/"%>

  <%= f.file_field :images, :multiple => true %>

  <%= f.submit %>

<% end %>
